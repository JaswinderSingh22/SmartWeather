{"ast":null,"code":"import { computed, ref } from 'vue';\nimport weatherCard from '../components/weatherCard.vue';\nimport searchBar from '../components/searchBar.vue';\nexport default {\n  components: {\n    weatherCard,\n    searchBar\n  },\n  async setup() {\n    const searchTerm = ref('');\n    const searchResults = ref([{\n      id: 1,\n      location: 'City 1',\n      temperature: '25°C',\n      description: 'Sunny'\n    }]);\n    const url = 'https://yahoo-weather5.p.rapidapi.com/weather?location=sunnyvale&format=json&u=f';\n    const options = {\n      method: 'GET',\n      headers: {\n        'X-RapidAPI-Key': '6f36563269msha5cb7301a8a407ep1e9a1ejsn152a060f2a28',\n        'X-RapidAPI-Host': 'yahoo-weather5.p.rapidapi.com'\n      }\n    };\n    try {\n      const response = await fetch(url, options);\n      const result = await response.text();\n      console.log(result);\n    } catch (error) {\n      console.error(error);\n    }\n    const filteredResults = computed(() => {\n      if (searchTerm.value) {\n        return searchResults.value.filter(result => result.location.toLowerCase().includes(searchTerm.value.toLowerCase()));\n      }\n      return [];\n    });\n    const handleSearch = value => {\n      searchTerm.value = value;\n    };\n    return {\n      searchTerm,\n      searchResults,\n      filteredResults,\n      handleSearch\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","weatherCard","searchBar","components","setup","searchTerm","searchResults","id","location","temperature","description","url","options","method","headers","response","fetch","result","text","console","log","error","filteredResults","value","filter","toLowerCase","includes","handleSearch"],"sources":["/Users/turbanator/Documents/CODING/FRONT-END/weather-app/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto py-8\">\n    <h1 class=\"text-4xl font-bold mb-4\">Weather Forecast</h1>\n    \n    <!-- Search Bar -->\n    <searchBar v-model=\"searchTerm\" @input=\"handleSearch\" />\n\n    <!-- Search Results -->\n    <div v-if=\"true\">\n      <h2 class=\"text-xl font-semibold\">Your Place</h2>\n      <div class=\"grid grid-cols-1 gap-4\">\n        <weatherCard\n          v-for=\"result in searchResults\"\n          :key=\"result.id\"\n          :location=\"result.location\"\n          :temperature=\"result.temperature\"\n          :description=\"result.description\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed, ref } from 'vue';\nimport weatherCard from '../components/weatherCard.vue';\nimport searchBar from '../components/searchBar.vue';\n\nexport default {\n  components: {\n    weatherCard,\n    searchBar,\n  },\n  async setup() {\n    const searchTerm = ref('');\n    const searchResults = ref([\n      { id: 1, location: 'City 1', temperature: '25°C', description: 'Sunny' },\n    ]);\n\n    const url = 'https://yahoo-weather5.p.rapidapi.com/weather?location=sunnyvale&format=json&u=f';\n    const options = {\n      method: 'GET',\n      headers: {\n        'X-RapidAPI-Key': '6f36563269msha5cb7301a8a407ep1e9a1ejsn152a060f2a28',\n        'X-RapidAPI-Host': 'yahoo-weather5.p.rapidapi.com'\n      }\n    };\n\n    try {\n      const response = await fetch(url, options);\n      const result = await response.text();\n      console.log(result);\n    } catch (error) {\n      console.error(error);\n    }\n\n    const filteredResults = computed(() => {\n      if (searchTerm.value) {\n        return searchResults.value.filter(result =>\n          result.location.toLowerCase().includes(searchTerm.value.toLowerCase())\n        );\n      }\n      return [];\n    });\n\n    const handleSearch = (value) => {\n      searchTerm.value = value;\n    };\n\n    return {\n      searchTerm,\n      searchResults,\n      filteredResults,\n      handleSearch,\n    };\n  },\n};\n</script>\n\n<style>\n.container {\n  max-width: 600px;\n}\n\n.grid-cols-2 {\n  grid-template-columns: repeat(2, minmax(0, 1fr));\n}\n</style>\n"],"mappings":"AAwBA,SAASA,QAAQ,EAAEC,GAAE,QAAS,KAAK;AACnC,OAAOC,WAAU,MAAO,+BAA+B;AACvD,OAAOC,SAAQ,MAAO,6BAA6B;AAEnD,eAAe;EACbC,UAAU,EAAE;IACVF,WAAW;IACXC;EACF,CAAC;EACD,MAAME,KAAKA,CAAA,EAAG;IACZ,MAAMC,UAAS,GAAIL,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMM,aAAY,GAAIN,GAAG,CAAC,CACxB;MAAEO,EAAE,EAAE,CAAC;MAAEC,QAAQ,EAAE,QAAQ;MAAEC,WAAW,EAAE,MAAM;MAAEC,WAAW,EAAE;IAAQ,CAAC,CACzE,CAAC;IAEF,MAAMC,GAAE,GAAI,kFAAkF;IAC9F,MAAMC,OAAM,GAAI;MACdC,MAAM,EAAE,KAAK;MACbC,OAAO,EAAE;QACP,gBAAgB,EAAE,oDAAoD;QACtE,iBAAiB,EAAE;MACrB;IACF,CAAC;IAED,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAACL,GAAG,EAAEC,OAAO,CAAC;MAC1C,MAAMK,MAAK,GAAI,MAAMF,QAAQ,CAACG,IAAI,CAAC,CAAC;MACpCC,OAAO,CAACC,GAAG,CAACH,MAAM,CAAC;IACrB,EAAE,OAAOI,KAAK,EAAE;MACdF,OAAO,CAACE,KAAK,CAACA,KAAK,CAAC;IACtB;IAEA,MAAMC,eAAc,GAAIvB,QAAQ,CAAC,MAAM;MACrC,IAAIM,UAAU,CAACkB,KAAK,EAAE;QACpB,OAAOjB,aAAa,CAACiB,KAAK,CAACC,MAAM,CAACP,MAAK,IACrCA,MAAM,CAACT,QAAQ,CAACiB,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACrB,UAAU,CAACkB,KAAK,CAACE,WAAW,CAAC,CAAC,CACvE,CAAC;MACH;MACA,OAAO,EAAE;IACX,CAAC,CAAC;IAEF,MAAME,YAAW,GAAKJ,KAAK,IAAK;MAC9BlB,UAAU,CAACkB,KAAI,GAAIA,KAAK;IAC1B,CAAC;IAED,OAAO;MACLlB,UAAU;MACVC,aAAa;MACbgB,eAAe;MACfK;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}