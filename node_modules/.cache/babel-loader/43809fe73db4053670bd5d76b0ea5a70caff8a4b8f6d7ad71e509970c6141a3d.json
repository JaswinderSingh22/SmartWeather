{"ast":null,"code":"import { computed, ref } from 'vue';\nimport WeatherCard from './components/WeatherCard.vue';\nimport SearchBar from './components/SearchBar.vue';\nexport default {\n  components: {\n    WeatherCard,\n    SearchBar\n  },\n  setup() {\n    const searchTerm = ref('');\n    const searchResults = ref([{\n      id: 1,\n      location: 'City 1',\n      temperature: '25°C',\n      description: 'Sunny'\n    }, {\n      id: 2,\n      location: 'City 2',\n      temperature: '20°C',\n      description: 'Cloudy'\n    }, {\n      id: 3,\n      location: 'City 3',\n      temperature: '15°C',\n      description: 'Rainy'\n    }]);\n    const filteredResults = computed(() => {\n      if (searchTerm.value) {\n        return searchResults.value.filter(result => result.location.toLowerCase().includes(searchTerm.value.toLowerCase()));\n      }\n      return [];\n    });\n    const handleSearch = value => {\n      searchTerm.value = value;\n    };\n    return {\n      searchTerm,\n      searchResults,\n      filteredResults,\n      handleSearch\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","WeatherCard","SearchBar","components","setup","searchTerm","searchResults","id","location","temperature","description","filteredResults","value","filter","result","toLowerCase","includes","handleSearch"],"sources":["/Users/turbanator/Documents/CODING/FRONT-END/weather-app/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto py-8\">\n    <h1 class=\"text-4xl font-bold mb-4\">Weather Forecast</h1>\n    \n    <!-- Search Bar -->\n    <SearchBar v-model=\"searchTerm\" @input=\"handleSearch\" />\n\n    <!-- Search Results -->\n    <div v-if=\"showResults\">\n      <h2 class=\"text-xl font-semibold\">Search Results:</h2>\n      <div class=\"grid grid-cols-2 gap-4\">\n        <WeatherCard\n          v-for=\"result in filteredResults\"\n          :key=\"result.id\"\n          :location=\"result.location\"\n          :temperature=\"result.temperature\"\n          :description=\"result.description\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed, ref } from 'vue';\nimport WeatherCard from './components/WeatherCard.vue';\nimport SearchBar from './components/SearchBar.vue';\n\nexport default {\n  components: {\n    WeatherCard,\n    SearchBar,\n  },\n  setup() {\n    const searchTerm = ref('');\n    const searchResults = ref([\n      { id: 1, location: 'City 1', temperature: '25°C', description: 'Sunny' },\n      { id: 2, location: 'City 2', temperature: '20°C', description: 'Cloudy' },\n      { id: 3, location: 'City 3', temperature: '15°C', description: 'Rainy' },\n    ]);\n\n    const filteredResults = computed(() => {\n      if (searchTerm.value) {\n        return searchResults.value.filter(result =>\n          result.location.toLowerCase().includes(searchTerm.value.toLowerCase())\n        );\n      }\n      return [];\n    });\n\n    const handleSearch = (value) => {\n      searchTerm.value = value;\n    };\n\n    return {\n      searchTerm,\n      searchResults,\n      filteredResults,\n      handleSearch,\n    };\n  },\n};\n</script>\n\n<style>\n.container {\n  max-width: 600px;\n}\n\n.grid-cols-2 {\n  grid-template-columns: repeat(2, minmax(0, 1fr));\n}\n</style>\n"],"mappings":"AAwBA,SAASA,QAAQ,EAAEC,GAAE,QAAS,KAAK;AACnC,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,UAAU,EAAE;IACVF,WAAW;IACXC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIL,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMM,aAAY,GAAIN,GAAG,CAAC,CACxB;MAAEO,EAAE,EAAE,CAAC;MAAEC,QAAQ,EAAE,QAAQ;MAAEC,WAAW,EAAE,MAAM;MAAEC,WAAW,EAAE;IAAQ,CAAC,EACxE;MAAEH,EAAE,EAAE,CAAC;MAAEC,QAAQ,EAAE,QAAQ;MAAEC,WAAW,EAAE,MAAM;MAAEC,WAAW,EAAE;IAAS,CAAC,EACzE;MAAEH,EAAE,EAAE,CAAC;MAAEC,QAAQ,EAAE,QAAQ;MAAEC,WAAW,EAAE,MAAM;MAAEC,WAAW,EAAE;IAAQ,CAAC,CACzE,CAAC;IAEF,MAAMC,eAAc,GAAIZ,QAAQ,CAAC,MAAM;MACrC,IAAIM,UAAU,CAACO,KAAK,EAAE;QACpB,OAAON,aAAa,CAACM,KAAK,CAACC,MAAM,CAACC,MAAK,IACrCA,MAAM,CAACN,QAA