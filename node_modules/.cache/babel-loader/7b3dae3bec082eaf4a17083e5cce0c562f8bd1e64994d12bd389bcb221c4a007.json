{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'SearchBar',\n  setup() {\n    const searchQuery = ref('');\n    const results = ref([]);\n    const store = useStore();\n    const fetchResults = async () => {\n      // Call weather API with searchQuery and update results\n      // Use store actions to fetch the results and update the store\n      // e.g., store.dispatch('fetchWeatherResults', searchQuery);\n    };\n    const selectResult = result => {\n      // Update selected city in store\n      // e.g., store.commit('selectCity', result);\n    };\n    return {\n      searchQuery,\n      results,\n      fetchResults,\n      selectResult\n    };\n  }\n};","map":{"version":3,"names":["ref","useStore","name","setup","searchQuery","results","store","fetchResults","selectResult","result"],"sources":["/Users/turbanator/Documents/CODING/FRONT-END/weather-app/src/components/searchBar.vue"],"sourcesContent":["<template>\n    <div class=\"flex items-center\">\n      <input v-model=\"searchQuery\" @input=\"fetchResults\" placeholder=\"Enter city or country\"\n             class=\"py-2 px-3 rounded-l-md focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-transparent\">\n      <ul v-if=\"results.length\"\n          class=\"absolute w-full bg-white shadow-md mt-1 rounded-b-md overflow-hidden z-10\">\n        <li v-for=\"result in results\" :key=\"result.id\" @click=\"selectResult(result)\"\n            class=\"px-3 py-2 cursor-pointer hover:bg-blue-100\">{{ result.name }}, {{ result.country }}</li>\n      </ul>\n    </div>\n  </template>\n  \n  <script>\n  import { ref } from 'vue';\n  import { useStore } from 'vuex';\n  \n  export default {\n    name: 'SearchBar',\n    setup() {\n      const searchQuery = ref('');\n      const results = ref([]);\n      const store = useStore();\n  \n      const fetchResults = async () => {\n        // Call weather API with searchQuery and update results\n        // Use store actions to fetch the results and update the store\n        // e.g., store.dispatch('fetchWeatherResults', searchQuery);\n      };\n  \n      const selectResult = (result) => {\n        // Update selected city in store\n        // e.g., store.commit('selectCity', result);\n      };\n  \n      return { searchQuery, results, fetchResults, selectResult };\n    },\n  };\n  </script>\n  "],"mappings":"AAaE,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,WAAU,GAAIJ,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMK,OAAM,GAAIL,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMM,KAAI,GAAIL,QAAQ,CAAC,CAAC;IAExB,MAAMM,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC/B;MACA;MACA;IAAA,CACD;IAED,MAAMC,YAAW,GAAKC,MAAM,IAAK;MAC/B;MACA;IAAA,CACD;IAED,OAAO;MAAEL,WAAW;MAAEC,OAAO;MAAEE,YAAY;MAAEC;IAAa,CAAC;EAC7D;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}